		{
			"name": "CloudTrail Demo APIs",
			"item": [
				{
					"name": "1. Create Cloudtrail Index Policy",
					"request": {
						"method": "PUT",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"policy\": {\n    \"description\": \"ingesting logs\",\n    \"default_state\": \"ingest\",\n    \"states\": [\n      {\n        \"name\": \"ingest\",\n        \"actions\": [\n          {\n            \"rollover\": {\n              \"min_doc_count\": 5\n            }\n          }\n        ],\n        \"transitions\": [\n          {\n            \"state_name\": \"search\"\n          }\n        ]\n      },\n      {\n        \"name\": \"search\",\n        \"actions\": [],\n        \"transitions\": [\n          {\n            \"state_name\": \"delete\",\n            \"conditions\": {\n              \"min_index_age\": \"5m\"\n            }\n          }\n        ]\n      },\n      {\n        \"name\": \"delete\",\n        \"actions\": [\n          {\n            \"delete\": {}\n          }\n        ],\n        \"transitions\": []\n      }\n    ]\n  }\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
							"raw": "{{endpoint}}/_opendistro/_ism/policies/hot_to_warm_policy",
							"host": [
								"{{endpoint}}"
							],
							"path": [
								"_opendistro",
								"_ism",
								"policies",
								"hot_to_warm_policy"
							]
						}
					},
					"response": []
				},
				{
					"name": "2. CreateCloudtrail Index Template",
					"request": {
						"method": "PUT",
						"header": [],
						"body": {
							"mode": "raw",
							"raw": "{\n  \"index_patterns\": [ \"cloudtrail-*\" ],\n  \"aliases\": {\n      \"cloudtrail\": {}\n    },\n    \"settings\": {\n      \"number_of_shards\": 1,\n      \"number_of_replicas\": 1,\n      \"opendistro.index_state_management.policy_id\": \"hot_to_warm_policy\"\n    }\n}",
							"options": {
								"raw": {
									"language": "json"
								}
							}
						},
						"url": {
